{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Adresse list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Gouvernorat</th>
                <th>Codepostal</th>
                <th>Ville</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for gouvernorat in gouvernorats %}
            <tr>
            
                <td>{{ gouvernorat.gouvernorat }}</td>
               
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('adresse_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
